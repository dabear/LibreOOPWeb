<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LibreOOPWeb</title>
    <link href="/Content/bootstrap.css" rel="stylesheet"/>
    <link href="/Content/site.css" rel="stylesheet"/>
    <link href="/Content/jquery.powertip.min.css" rel="stylesheet"/>
        
    <script src="/Scripts/modernizr-2.8.3.js"></script>
    <script src="/scripts/prettyprint.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("LibreOOPAlgoWeb", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Upload raw libre reading", "UploadRawReading", "Home")</li>
                    <li>@Html.ActionLink("Calibrate Sensor", "CalibrateSensor", "Home")</li>
                    <!--<li>@Html.ActionLink("Go to CustomShareServer Login", "Index", "Token")</li>
    <li>@Html.ActionLink("Get glucose", "Index", "Glucose")</li>
    <li>@Html.ActionLink("View logs", "ViewLogs", "Home")</li> -->
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer></footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Content/jquery.powertip.min.js")
    @RenderSection("scripts", required: false)
</body>
</html>
